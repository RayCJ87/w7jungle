<tr>
  <td>
    <%= image_tag product.image.tiny %>
  </td>
  <td>
    <h4><%= product.name %></h4>
    <br>
    <p><%= product.description %></p>
  </td>
  <td>
    <%= product.price %>
  </td>
  <td>
    <%= quantity%>
  </td>
  <td>
    <%= product.price_cents * quantity / 100.00 %>
  </td>
</tr>
